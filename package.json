{
  "name": "alunos_frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src --ext ts,tsx,js,jsx",
